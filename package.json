{
  "name": "supa-sailrs-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "build:serviceWorker": "esbuild ./app/serviceWorker.ts --bundle --minify --format=esm --define:process.env.NODE_ENV='\"production\"' --outfile=./public/serviceWorker.js",
    "db:prepare-migration": "prisma migrate dev --create-only --skip-seed",
    "db:deploy-migration": "prisma migrate deploy",
    "otherDev": "remix dev -c \"node ./build/server.js\"",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_OPTIONS=\"--require ./mocks --require dotenv/config\" remix dev -c \"node ./build/server.js\"",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:serviceWorker": "esbuild ./app/serviceWorker.ts --outfile=./public/serviceWorker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "format": "prettier --write .",
    "generate:graphql": "dotenv -- graphql-codegen --config codegen.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "run-s setup:db setup:seed",
    "setup:db": "prisma generate && prisma migrate deploy",
    "setup:seed": "prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 'npx cypress open'",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "dotenv -- cross-env PORT=8811 start-server-and-test http://localhost:8811 'npx cypress run'",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "@graphql-codegen/typescript-apollo-client-helpers": "2.2.6",
    "@graphql-tools/schema": "^9.0.18",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@prisma/client": "^4.10.1",
    "@remix-run/css-bundle": "*",
    "@remix-run/express": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "@sailrs/pwa": "^0.3.3",
    "@supabase/supabase-js": "^2.26.0",
    "class-variance-authority": "^0.7.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-backend": "^1.4.1",
    "isbot": "^3.5.0",
    "morgan": "^1.10.0",
    "postgraphile": "^4.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.5",
    "react-zorm": "^0.6.1",
    "remix-i18next": "^4.1.1",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^1.13.2",
    "tiny-invariant": "^1.3.1",
    "web-push": "^3.6.3",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/add": "3.2.3",
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.8",
    "@graphql-codegen/typescript-generic-sdk": "^3.1.0",
    "@graphql-codegen/typescript-operations": "2.5.13",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.40.2",
    "@types/express": "^4.17.17",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "cookie": "^0.5.0",
    "cypress": "^12.17.1",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "happy-dom": "^10.1.1",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.4.0",
    "prisma": "^4.10.1",
    "start-server-and-test": "^2.0.0",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "schema": "app/integrations/prisma/schema.prisma",
    "seed": "ts-node --require tsconfig-paths/register app/integrations/prisma/seed.server.ts"
  }
}
